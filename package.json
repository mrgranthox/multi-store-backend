{
  "name": "multi-store-ecommerce-back4app",
  "version": "1.0.0",
  "description": "Production-ready Node.js + TypeScript backend for Back4App multi-store e-commerce",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "seed": "ts-node src/prisma/seed.ts",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "postman": "newman run postman/collection.json -e postman/environment.json",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "reconcile:all": "ts-node src/scripts/reconcileAll.ts"
  },
  "keywords": [
    "ecommerce",
    "multi-store",
    "back4app",
    "strapi",
    "prisma",
    "typescript"
  ],
  "author": "Multi-Store E-commerce Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@sentry/node": "^10.11.0",
    "@sentry/tracing": "^7.120.4",
    "@types/bcrypt": "^6.0.0",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "newman": "^6.1.0",
    "prettier": "^3.1.0",
    "prisma": "^6.16.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
 
}
